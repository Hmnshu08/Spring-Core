<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
<bean id="questions" class="com.spring.collections.collectionEx.Questions" >
       <property name="questionId">
       		<list>
       			<value>1</value>
       			<value>2</value>
       			<value>3</value>
       		</list>
       
       </property>
       
       <property name="question">
	       	<set>
		       	<value>What is 1+1</value>
		       	<value>What is 2+1</value>
		       	<value>What is 1+6</value>
	       	</set>
       </property>

		<property name="answers">
			<map>
				<entry key = "1" value= "2"></entry>
				<entry key = "2" value= "3"></entry>
				<entry key = "3" value= "7"></entry>
			</map>
		</property>

</bean>
       
       
       
       
</beans>